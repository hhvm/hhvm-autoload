{
  "name": "hhvm/hhvm-autoload",
  "replace": {
    "facebook/hhvm-autoload": "1.*"
  },
  "type": "composer-plugin",
  "bin": [
    "bin/hh-autoload",
    "bin/hh-autoload.hack"
  ],
  "require": {
    "composer-plugin-api": "^1.0",
    "hhvm": "^4.25",
    "hhvm/hsl": "^4.0"
  },
  "require-dev": {
    "hhvm/hacktest": "^2.0",
    "facebook/fbexpect": "^2.1"
  },
  "autoload": {
    "classmap": [
      "ComposerPlugin.php"
    ]
  },
  "extra": {
    "class": [
      "Facebook\\AutoloadMap\\ComposerPlugin"
    ],
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
